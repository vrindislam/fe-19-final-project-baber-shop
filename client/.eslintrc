{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true
  },
  "extends": [
    "standard",
    "react-app",
    "prettier"
  ],
  "rules": {
    "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
    "no-return-assign": 0,
    "eol-last": 0,
    "semi": 0,
    "camelcase": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0
  },
  "plugins": [
    "prettier"
  ],
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "16.7.0",
      "flowVersion": "0.86"
    },
    "propWrapperFunctions": [ "forbidExtraProps" ]
  }
}